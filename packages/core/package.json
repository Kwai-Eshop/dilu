    "core-js": "^3.12.1",
    "eslint": "^6.8.0",
    "qiankun": "2.10.3",
    "rollup": "^2.47.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "terser": "^4.6.13",
    "ts-jest": "^29.0.5",
    "tsc-alias": "^1.8.3",
    "typescript": "^4.3.5"
  },
  "gitHead": "b7b7c0aff4510e5a59f22d10a8c01959f80cd363",
